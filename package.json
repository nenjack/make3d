{
  "name": "make3d",
  "description": "make3d - ddd retro boomer game engine",
  "version": "1.1.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "chef-express docs",
    "dev": "yarn build && yarn start",
    "docs": "yarn build",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "yarn build:docs",
    "prebuild:docs": "rimraf docs",
    "build:docs": "typedoc --customCss typedoc.css",
    "postbuild:docs": "prettier docs --write && yarn docs-from-master",
    "docs-from-master-macos": "find docs -type f -name '*.html' -exec sed -i '' -r 's_/blob/[^/]+/_/blob/main/_g' {} +",
    "docs-from-master-linux": "find docs -type f -name '*.html' -exec sed -i -r 's_/blob/[^/]+/_/blob/main/_g' {} +",
    "docs-from-master": "yarn docs-from-master-macos || yarn docs-from-master-linux",
    "lint": "eslint 'src/**/*.ts' --fix",
    "format": "prettier . --write",
    "precommit": "yarn lint && yarn build && yarn format",
    "amend": "yarn precommit && git commit -a --am --no-edit"
  },
  "files": [
    "dist/*.*"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-typescript": "^7.28.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/three": "^0.181.0",
    "check2d": "^9.33.0",
    "chef-express": "^3.2.3",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "pixi-stats": "^5.0.3",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "rimraf": "^6.1.0",
    "rot-js": "^2.2.1",
    "three": "^0.181.2",
    "tslint": "^6.1.3",
    "typedoc": "^0.28.14",
    "typescript": "^5",
    "typescript-eslint": "^8.47.0"
  },
  "keywords": [
    "unity",
    "gamedev",
    "gameobject",
    "prefab",
    "state",
    "machine",
    "sprite",
    "circle",
    "body",
    "polygon",
    "physics",
    "container",
    "scene",
    "animator",
    "webgl",
    "threejs"
  ],
  "repository": "git://github.com/Jackie-Aniki/make3d.git",
  "bugs": {
    "url": "https://github.com/Jackie-Aniki/make3d/issues"
  },
  "homepage": "https://jackie-aniki.github.io/make3d/"
}
