{
  "name": "engine25d",
  "description": "threejs based game engine",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "chef-express docs --port 3000",
    "docs": "yarn build && yarn start",
    "build": "tsc",
    "postbuild": "yarn build:docs",
    "prebuild:docs": "rm -rf docs",
    "build:docs": "typedoc --customCss typedoc.css",
    "postbuild:docs": "prettier docs --write && yarn docs-from-master",
    "docs-from-master-macos": "find docs -type f -name '*.html' -exec sed -i '' -r 's_/blob/[^/]+/_/blob/main/_g' {} +",
    "docs-from-master-linux": "find docs -type f -name '*.html' -exec sed -i -r 's_/blob/[^/]+/_/blob/main/_g' {} +",
    "docs-from-master": "yarn docs-from-master-macos || yarn docs-from-master-linux",
    "lint": "eslint 'src/**/*.ts' --fix",
    "format": "prettier . --write",
    "test": "jest --silent --verbose --forceExit --passWithNoTests",
    "precommit": "yarn lint && yarn build && yarn test && yarn format",
    "amend": "yarn precommit && git commit -a --am --no-edit"
  },
  "files": [
    "src/*.*"
  ],
  "license": "MIT",
  "dependencies": {
    "detect-collisions": "^9.24.0",
    "inject.min": "^1.6.4",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/offscreencanvas": "^2019.7.3",
    "@types/rot-js": "^2.0.1",
    "@types/three": "^0.172.0",
    "babel-jest": "^29.7.0",
    "chef-express": "^2.4.2",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "29.7.0",
    "pixi-stats": "^4.0.0",
    "prettier": "^3.4.2",
    "rot-js": "^2.2.1",
    "three": "^0.172.0",
    "ts-loader": "^9.5.2",
    "typedoc": "^0.27.6",
    "typescript": "^5",
    "typescript-eslint": "^8.21.0"
  },
  "keywords": [
    "unity",
    "gamedev",
    "gameobject",
    "prefab",
    "state",
    "machine",
    "sprite",
    "circle",
    "body",
    "polygon",
    "physics",
    "container",
    "scene",
    "animator",
    "webgl",
    "threejs"
  ],
  "repository": "git://github.com/Prozi/engine25d.git",
  "bugs": {
    "url": "https://github.com/Prozi/engine25d/issues"
  },
  "homepage": "https://prozi.github.io/engine25d/"
}
